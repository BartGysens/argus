/* Supporting function */
/**
 * Output html used as model for generating actions
 *        	
 */
function argus_beleidsplannen_model_action($body = '', $deadline = NULL, $executor = NULL, $budget = 0, $status = 'nieuw', $rok_opts = array(), $aid = '{aid}', $ref = '{ref}', $cntr = '{cntr}', $class = '{class}') {
	global $user;
	
	if (is_null ( $executor )) {
		$executor = $user->uid;
	}
	
	$executor_roles = array (
			88,
			87,
			225 
	);
	$query = 'SELECT ur.uid FROM {users_roles} AS ur INNER JOIN {users} AS u ON u.uid = ur.uid INNER JOIN {field_data_field_user_sms_naam} AS un ON un.entity_id = ur.uid INNER JOIN {field_data_field_user_sms_voornaam} AS uf ON uf.entity_id = ur.uid WHERE ur.rid in (:rids) AND u.status = 1 ORDER BY un.field_user_sms_naam_value ASC, uf.field_user_sms_voornaam_value ASC';
	$executors = db_query ( $query, array (
			':rids' => $executor_roles 
	) )->fetchCol ();
	
	if (is_null ( $deadline )) {
		$deadline = format_date ( time (), 'custom', 'd/m/Y' );
	}
	
	return theme ( 'edit-model-action', array (
			'body' => $body,
			'deadline' => $deadline,
			'ref' => $ref,
			'aid' => $aid,
			'status' => $status,
			'rok_opts' => $rok_opts,
			'executors' => $executors,
			'executor' => $executor,
			'budget' => $budget,
			'cntr' => $cntr,
			'class' => $class
	) );
}