/**
 * Hook preprocess for node
 *
 * @param type $variables        	
 */
function argus_beleidsplannen_preprocess_node(&$variables) {
	switch ($variables ['type']) {
		case 'beleidsplan' :
			if (user_access ( 'access argus_beleidsplannen content' )) {
				
				drupal_add_css ( drupal_get_path ( 'module', 'argus_beleidsplannen' ) . '/css/argus_beleidsplannen.css' );
				drupal_add_library ( 'system', 'drupal.collapse' );
				
				/* Set some global parameters */
				$node = $variables ['elements'] ['#node'];
				$variables ['node'] = $node;
				$variables ['nid'] = $node->nid;
				
				$params = explode ( '/', current_path () );
				if (array_key_exists ( 2, $params )) {
					$modus = $params [2];
				} else {
					$modus = '';
				}
				$variables ['modus'] = $modus;
				
				switch ($modus) {
					case 'actieplan' :
						drupal_add_js ( drupal_get_path ( 'module', 'argus_beleidsplannen' ) . '/js/argus_beleidsplannen_actieplan.js' );
						drupal_add_library ( 'system', 'ui.datepicker' );
				
						if (array_key_exists ( 'submit', $_POST )) {
								
							for ($cntr = 0; $cntr < $_POST['argus_beleidsplannen_cntr']; $cntr++){
								if (array_key_exists('argus_beleidsplannen_' . $cntr . '_aid', $_POST)){
									$aid = $_POST ['argus_beleidsplannen_' . $cntr . '_aid'];
										
									$delete_action = false;
									if (is_numeric ( $aid )) {
										if ($_POST ['argus_beleidsplannen_' . $cntr . '_action_description'] == '{delete}') {
											node_delete ( $aid );
											$delete_action = true;
										} else {
											$action = node_load ( $aid );
										}
									} else {
										$action = new stdClass ();
										$action->type = 'actie';
										$action->language = LANGUAGE_NONE;
										$action->status = 1;
										$action->promote = 0;
										$action->comment = 0;
										node_object_prepare ( $action );
											
										global $user;
										$action->uid = $user->uid;
										$action->field_referentie [LANGUAGE_NONE] [0] ['value'] = $node->nid . '-' . $_POST ['argus_beleidsplannen_' . $cntr . '_ref'] . '-' . $cntr;
										$action->field_beleidsplan [LANGUAGE_NONE] [0] ['target_id'] = $node->nid;
											
										/* Send user a Smartschool message (only new actions) */
										if (module_exists ( 'argus_sms_api' )) {
											$body = '<u>Herinnering</u><br /><br />Betreft: <strong>ACTIE BELEIDSPLAN ' . $node->title . '</strong><br />Leerling: <strong>' . argus_get_user_realname ( $_POST ['argus_beleidsplannen_' . $cntr . '_action_executor'] ) . '</strong><br />Te doen: <strong>' . $_POST ['argus_beleidsplannen_' . $cntr . '_action_description'] . '</strong><br />';
											// argus_sms_api_send_msg ( 'argus_beleidsplannen_new_action', $_POST ['argus_beleidsplannen_' . $ref . '_action_executor'] [$k], 0, $user->uid, 'Nieuwe actie aangemaakt voor beleidsplan: ' . $node->title, $body );
										}
									}
										
									if (!$delete_action){
										$action->body [LANGUAGE_NONE] [0] ['value'] = $_POST ['argus_beleidsplannen_' . $cntr . '_action_description'];
										$action->field_deadline [LANGUAGE_NONE] [0] ['value'] = date_format(date_create_from_format('d/m/Y', $_POST ['argus_beleidsplannen_'.$cntr.'_action_deadline']), 'Y-m-d');
										$action->field_status [LANGUAGE_NONE] [0] ['value'] = $_POST ['argus_beleidsplannen_' . $cntr . '_action_status'];
										$action->field_bedrag [LANGUAGE_NONE] [0] ['value'] = $_POST ['argus_beleidsplannen_' . $cntr . '_action_budget'];
										$action->field_gebruiker [LANGUAGE_NONE] [0] ['target_id'] = $_POST ['argus_beleidsplannen_' . $cntr . '_action_executor'];
										$action->title = 'Actie voor ' . argus_get_user_realname( $_POST ['argus_beleidsplannen_' . $cntr . '_action_executor'] ) . ' ihkv. het beleidsplan "' . $node->title . '" tegen ' . $_POST ['argus_beleidsplannen_'.$cntr.'_action_deadline'];
				
										$k = 0;
										$c = 0;
										foreach (list_allowed_values( field_info_field( 'field_integratie_r_ok' ) ) as $o ){
											if (array_key_exists('argus_beleidsplannen_'. $cntr . '_action_rok_'. $k, $_POST)){
												if ($_POST['argus_beleidsplannen_'. $cntr . '_action_rok_'. $k ]){
													$action->field_integratie_r_ok [LANGUAGE_NONE] [$c] ['value'] = $o;
													$c++;
												}
											}
											$k++;
										}
				
										node_save ( $action );
									}
								}
							}
						}
				
						break;
					default:
						drupal_add_js ( "https://www.google.com/jsapi?autoload={'modules':[{'name':'visualization','version':'1','packages':['corechart']}]}" );
						drupal_add_js ( drupal_get_path ( 'module', 'argus_beleidsplannen' ) . '/js/argus_beleidsplannen.js' );
				}
				
				$variables ['actions'] = array ();
				$variables ['action_plan'] = array();
				foreach ( $variables ['field_beleidsplan_op_doelst'] as $k => $d ) {
					$ref = md5 ( $d ['value'] );
					$query = 'SELECT r.entity_id AS id, field_referentie_value AS ref FROM {field_data_field_referentie} AS r INNER JOIN {field_data_field_deadline} AS d ON d.entity_id = r.entity_id WHERE r.field_referentie_value LIKE :od ORDER BY d.field_deadline_value ASC';
					$results = db_query ( $query, array (
							':od' => $node->nid . '-' . $ref . '-%' 
					) )->fetchAllKeyed( 0, 1 );
					
					$variables ['actions'][$ref] = array();
					foreach ($results as $aid => $aref){
						$action = node_load($aid);
						
						$a = array(
								'id' => $aid,
								'description' => $action->body [LANGUAGE_NONE] [0] ['value'],
								'gebruiker' => $action->field_gebruiker [LANGUAGE_NONE] [0] ['target_id'],
								'budget' => $action->field_bedrag [LANGUAGE_NONE] [0] ['value'],
								'status' => $action->field_status [LANGUAGE_NONE] [0] ['value'],
								'deadline' => format_date(strtotime($action->field_deadline [LANGUAGE_NONE] [0] ['value']),'custom', 'd/m/Y'),
								'cntr' => $k+1,
								'rok_opt' => array(),
						);
						
						if (array_key_exists(LANGUAGE_NONE, $action->field_integratie_r_ok)){
							foreach($action->field_integratie_r_ok[LANGUAGE_NONE] as $rok_opt){
								$a['rok_opt'][] = $rok_opt['value'];
							}
						}
						
						$variables ['actions'][$ref][] = $a;
						
						$variables ['action_plan'][$action->field_deadline [LANGUAGE_NONE] [0] ['value']][] = $a;
					}
				}
				
				ksort($variables ['action_plan']);
				
				// Preprocess fields.
				field_attach_preprocess ( 'node', $node, $variables ['elements'], $variables );
			} else {
				drupal_access_denied ();
				exit ();
			}
			break;
	}
}

/**
 * Page callback mapped to the url /beleidsplannen/structure
 *
 * @return array
 */
function argus_beleidsplannen_structure() {
	drupal_add_css ( drupal_get_path ( 'module', 'argus_beleidsplannen' ) . '/css/argus_beleidsplannen.css' );
	
	/* Load Google Charts API */
	drupal_add_js ( "https://www.gstatic.com/charts/loader.js" );
	
	drupal_add_js ( drupal_get_path ( 'module', 'argus_beleidsplannen' ) . '/js/argus_beleidsplannen_structure.js' );
	
	$data = array ();
	
	$query = 'SELECT n.nid AS id, p.field_beleidsplan_overkoepelend_target_id AS parent, n.title AS title, ikznr.field_ikz_nummer_value AS ikznr FROM {node} AS n
			LEFT JOIN {field_data_field_ikz_nummer} AS ikznr ON ikznr.entity_id = n.nid 
			LEFT JOIN {field_data_field_beleidsplan_overkoepelend} AS p ON p.entity_id = n.nid 
			WHERE n.type = :type ORDER BY p.field_beleidsplan_overkoepelend_target_id ASC
			';
	$results = db_query ( $query, array (
			':type' => 'beleidsplan' 
	) )->fetchAllAssoc ( 'id', PDO::FETCH_ASSOC );
	
	foreach ( $results as $id => $bp ) {
		$data [] = array (
				array (
						'v' => $bp ['id'],
						'f' => '<a href="' . drupal_get_path_alias ( '/node/' . $bp ['id'] ) . '">' . $bp ['title'] . '</a><br />IKZ-rev. ' . $bp ['ikznr'] 
				),
				$bp ['parent'],
				$bp ['title'] 
		);
	}
	
	return theme ( 'argus_beleidsplannen_structure', array (
			'data' => json_encode ( $data ) 
	) );
}

/**
 * Page callback mapped to the url /jaaractieplan
 *
 * @return array
 */
function argus_beleidsplannen_jap() {
	drupal_add_css ( drupal_get_path ( 'module', 'argus_beleidsplannen' ) . '/css/argus_beleidsplannen.css' );
	
	// Get all beleidsplannen
	$query = 'SELECT n.nid AS id, n.title AS title, od.field_beleidsplan_op_doelst_value AS od FROM {node} AS n INNER JOIN {field_data_field_beleidsplan_op_doelst} AS od ON od.entity_id = n.nid WHERE n.type = :type';
	$results = db_query ( $query, array (
			':type' => 'beleidsplan',
	) )->fetchAll();
	
	$variables ['actions'] = array();
	$cntr = 0;
	
	$schoolyear = 2018;
	
	$variables ['startdate'] = 0;
	$variables ['enddate'] = 0;
	foreach ($results as $bp){
		$query = 'SELECT r.entity_id AS id, field_referentie_value AS ref FROM {field_data_field_referentie} AS r INNER JOIN {field_data_field_deadline} AS d ON d.entity_id = r.entity_id WHERE r.field_referentie_value LIKE :od ORDER BY d.field_deadline_value ASC';
		$results = db_query ( $query, array (
				':od' => $bp->id . '-' . $ref . '-%',
		) )->fetchAllKeyed( 0, 1 );
		
		// Get all actions
		$ref = md5 ( $bp->od );
		$query = 'SELECT r.entity_id AS id, field_referentie_value AS ref FROM {field_data_field_referentie} AS r INNER JOIN {field_data_field_deadline} AS d ON d.entity_id = r.entity_id WHERE r.field_referentie_value LIKE :od AND d.field_deadline_value BETWEEN :startdate AND :enddate ORDER BY d.field_deadline_value ASC';
		$results = db_query ( $query, array (
				':od' => $bp->id . '-' . $ref . '-%',
				':startdate' => $schoolyear . '-09-01',
				':enddate' => ($schoolyear+1) . '-09-01',
		) )->fetchAllKeyed( 0, 1 );
		
		foreach ($results as $aid => $aref){
			$action = node_load($aid);
			
			$a = array(
					'id' => $aid,
					'bp_id' => $bp->id,
					'bp_title' => $bp->title,
					'description' => $action->body [LANGUAGE_NONE] [0] ['value'],
					'gebruiker' => $action->field_gebruiker [LANGUAGE_NONE] [0] ['target_id'],
					'budget' => $action->field_bedrag [LANGUAGE_NONE] [0] ['value'],
					'status' => $action->field_status [LANGUAGE_NONE] [0] ['value'],
					'deadline' => format_date(strtotime($action->field_deadline [LANGUAGE_NONE] [0] ['value']),'custom', 'd/m/Y'),
					'cntr' => $cntr++,
					'od' => $bp->od,
					'rok_opt' => array(),
			);
			
			if (array_key_exists(LANGUAGE_NONE, $action->field_integratie_r_ok)){
				foreach($action->field_integratie_r_ok[LANGUAGE_NONE] as $rok_opt){
					$a['rok_opt'][] = $rok_opt['value'];
				}
			}
			
			$variables ['actions'][$action->field_deadline [LANGUAGE_NONE] [0] ['value']][] = $a;
		}
	}
	
	ksort($variables ['actions']);
	
	return theme ( 'argus_beleidsplannen_jap', $variables );
}

