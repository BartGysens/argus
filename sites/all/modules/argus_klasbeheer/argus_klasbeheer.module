/**
 * Retrieve all active classes with pupils.
 *
 * Function returns an array
 *        	
 * @return array int
 */
function argus_klasbeheer_get_active_classes() {
	global $user;
	if ( $cache = cache_get ( 'argus_klasbeheer_get_active_classes_' . $user->uid ) ) {
		$result = $cache->data;
	} else {
		$query = 'SELECT n.nid AS id, n.title ';
		$query .= 'FROM {node} AS n ';
		$query .= 'INNER JOIN {field_data_field_klas_leerlingen} AS lln ON lln.entity_id = n.nid ';
		$query .= 'WHERE type = :type ';
		$query .= 'GROUP BY n.nid HAVING COUNT(*) > 0 ';
		$query .= 'ORDER BY n.title ASC';
		$result = db_query ( $query, array (
					':type' => 'klas' 
			) )->fetchAllKeyed();
		
		cache_set ( 'argus_klasbeheer_get_active_classes_' . $user->uid, $result );
	}
	
	return $result;
}

/**
 * Retrieve the teachers of this class.
 *
 * Function returns an array
 *
 * @param $cid int
 *        	Node id
 *        	
 * @return array int
 */
function argus_klasbeheer_get_pupils($cid) {
	if (isset ( $cid )) {
		global $user;
		if ( $cache = cache_get ( 'argus_klasbeheer_get_pupils_' . $user->uid . '_' . $cid ) ) {
			$result = $cache->data;
		} else {
			$query = 'SELECT u.uid AS id ';
			$query .= 'FROM {field_data_field_klas_leerlingen} AS l ';
			$query .= 'INNER JOIN {users} AS u ON l.field_klas_leerlingen_target_id = u.uid ';
			$query .= 'LEFT JOIN {field_data_field_user_sms_voornaam} AS uv ON uv.entity_id = u.uid ';
			$query .= 'LEFT JOIN {field_data_field_user_sms_naam} AS un ON un.entity_id = u.uid ';
			$query .= 'WHERE l.entity_id = :cid ';
			//$query .= 'ORDER BY un.field_user_sms_naam_value ASC, uv.field_user_sms_voornaam_value ASC';
			$result = db_query ( $query, array (
					':cid' => $cid 
			) )->fetchCol();
			
			cache_set ( 'argus_klasbeheer_get_pupils_' . $user->uid . '_' . $cid, $result );
		}
		
		return $result;
	}
}

